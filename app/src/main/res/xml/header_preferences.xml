<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Preference
        app:fragment="com.pt.begawanpolosoro.setting.SettingsActivity$MessagesFragment"
        app:icon="@drawable/messages"
        app:key="messages_header"
        app:title="Edit Profil" />

    <Preference
        app:fragment="com.pt.begawanpolosoro.setting.SettingsActivity$SyncFragment"
        app:icon="@drawable/sync"
        app:key="sync_header"
        app:title="Ganti Password" />

    <Preference
        app:fragment="com.pt.begawanpolosoro.setting.SettingsActivity$Reset"
        app:icon="@drawable/ic_logout"
